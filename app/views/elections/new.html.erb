<h3>Create New Election Event</h3>
<%= form_with(model: @election, :url => election_path , local: true) do |f| %>

  <div class="form-group">
    <%= f.label :start_time %>
    <%= f.text_field :start_time, autofocus: true, placeholder: 'Enter New Halka Name' ,autocomplete: 'name', class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :end_time %>
    <%= f.text_field :end_time, autofocus: true, placeholder: 'Enter New Halka Name' ,autocomplete: 'name', class: 'form-control' %>
  </div>

  <div class="actions">
    <%= f.submit "Create", class: "btn btn-primary" %>
  </div>

<% end %>
